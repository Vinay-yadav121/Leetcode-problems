class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> s(nums1.begin(), nums1.end());
        vector<int> result;

        for (int num : nums2) {
            if (s.find(num) != s.end()) {
                result.push_back(num);   //  push matched element
                s.erase(num);            // erase to ensure uniqueness
            }
        }

        return result;
    }
};
