class Solution {
public:
    int maximumPopulation(vector<vector<int>>& logs) {
       
        int population[101] = {0};

        for (auto& log : logs) {
            int birth = log[0];
            int death = log[1];
            population[birth - 1950]++;   // Person born
            population[death - 1950]--;   // Person died
        }

        int maxPop = 0, maxYear = 1950, currPop = 0;
        for (int i = 0; i < 101; ++i) {
            currPop += population[i];
            if (currPop > maxPop) {
                maxPop = currPop;
                maxYear = 1950 + i;
            }
        }

        return maxYear;
        
    }
};
